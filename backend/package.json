{
  "name": "Ancesora",
  "version": "0.0.1",
  "description": "Ancesora",
  "author": "wintsa",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "copy-static-files": "node copy-static-files.js",
    "build": "nest build && node copy-static-files",
    "newSql": "npx prisma db push && npx prisma generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env RUNNING_ENV=dev nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js   --watch",
    "dev": "cross-env RUNNING_ENV=dev nest start --watch ",
    "debug": "cross-env RUNNING_ENV=dev nest start --debug --watch ",
    "prod": "cross-env RUNNING_ENV=prod node dist/main ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:prod": "nest build &&  copy-static-files && cross-env RUNNING_ENV=prod pm2 start ecosystem.config.js --env production",
    "pm2:dev": "nest build && node copy-static-files && pm2 start ecosystem.config.js --env development",
    "pm2:test": "nest build && node copy-static-files && pm2 start ecosystem.config.js --env test",
    "update": "git pull  && nest build && node copy-static-files && &&pm2 restart zw-nestjs &&pm2 log",
    "gitpush": "git add . && git commit -m \"backup  %DATE% %TIME%\" && git push"
  },
  "dependencies": {
