{
  "name": "Ancesora",
  "version": "0.0.1",
  "description": "Ancesora",
  "author": "wintsa",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "copy-static-files": "node copy-static-files.js",
    "build": "nest build && node copy-static-files",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env RUNNING_ENV=dev nest start",
    "dev": "cross-env RUNNING_ENV=dev nest start --watch ",
    "debug": "cross-env RUNNING_ENV=dev nest start --debug --watch ",
    "prod": "cross-env RUNNING_ENV=prod node dist/main ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:prod": "nest build &&  copy-static-files && cross-env RUNNING_ENV=prod pm2 start ecosystem.config.js --env production",
    "pm2:dev": "nest build && node copy-static-files && pm2 start ecosystem.config.js --env development",
    "pm2:test": "nest build && node copy-static-files && pm2 start ecosystem.config.js --env test",
    "update": "git pull  && nest build && node copy-static-files && &&pm2 restart zw-nestjs &&pm2 log",
    "gitpush": "git add . && git commit -m \"backup  %DATE% %TIME%\" && git push"
  },
  "dependencies": {
    "@blazity/nest-file-fastify": "^1.0.0",
    "@fastify/cookie": "^9.4.0",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/static": "^6.12.0",
    "@fastify/swagger": "^8.15.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.4.15",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/websockets": "^10.4.15",
    "@prisma/client": "6.2.1",
    "@upstash/redis": "^1.34.3",
    "ali-oss": "^6.22.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compressing": "^1.10.1",
    "cron": "^3.5.0",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "fast-xml-parser": "^4.5.1",
    "fastify-xml-body-parser": "^2.2.0",
    "form-data": "^4.0.1",
    "fs": "0.0.1-security",
    "fs-extra": "^11.3.0",
    "iconv-lite": "^0.6.3",
    "ioredis": "^5.4.2",
    "is-stream": "^4.0.1",
    "jsdom": "^24.1.3",
    "json-bigint": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.2.1",
    "promise-pool-ext": "^3.3.0",
    "promise-retry": "^2.0.1",
    "pump": "^3.0.2",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "serve-static": "^1.16.2",
    "socket.io": "^4.8.1",
    "svg-captcha": "^1.4.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xml2js": "^0.6.2",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20.17.14",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cz-customizable": "^7.4.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
